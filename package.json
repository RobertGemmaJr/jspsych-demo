    "jspsych": "7.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.4",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^9.0.10",
    "lint-staged": "^15.0.2",
    "prettier": "^3.2.5",
    "vite": "^5.2.11"
